{
    "name": "vinkla/algolia",
    "description": "An Algolia bridge for Laravel",
    "keywords": [
        "laravel",
        "algolia",
        "search",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@doubledip.se",
            "homepage": "https://doubledip.se"
        }
    ],
    "require": {
        "php": "^7.2",
        "algolia/algoliasearch-client-php": "^2.3",
        "graham-campbell/manager": "^4.3",
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.3",
        "graham-campbell/testbench": "^5.3",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.3"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Algolia\\AlgoliaServiceProvider"
            ],
            "aliases": {
                "Algolia": "Vinkla\\Algolia\\Facades\\Algolia"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Algolia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Algolia\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
